<h1>list of users</h1>

<table class="table table-condensed table-hover">
  <thead>
    <tr>
      <td>Name</td>
      <td>Registered Date</td>
      <td>Steps</td>
      <td>Calories</td>
    </tr>
  </thead>

  <tbody>
    {{#users}}
    <tr>
      <td>{{name}}</td>
      <td>{{createdAt}}</td>
      <td>{{steps}}</td>
      <td>{{calories}}</td>
    </tr>
    {{/users}}
  </tbody>

</table>
